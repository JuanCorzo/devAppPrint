<?xml version='1.0' encoding='utf-8' ?>
<widget id="com.example.HS" 
		version="1.0.0" 
		xmlns="http://www.w3.org/ns/widgets" 
		xmlns:gap="https://phonegap.com/ns/1.0" >

<name>Dev App Print</name>

<description>
    Impresion de ticketes para transferencias de pasajeros
</description>

<author email="soporte@lmsoluciones.co" href="http://www.lmsoluciones.co">
    LM SOLUCIONES
</author>

<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
  xmlns:android="http://schemas.android.com/apk/res/android"
  id="com.badrit.PrintPlugin"
  version="0.1.0">
  <name>PrintPlugin</name>
  <description>Cordova Print Plugin</description>
  <license>Apache 2.0</license>
  <keywords>cordova,PrintPlugin</keywords>

  <dependency id="org.apache.cordova.device" url="https://github.com/apache/cordova-plugin-device.git" />
  <dependency id="org.apache.cordova.inappbrowser" url="https://github.com/apache/cordova-plugin-inappbrowser.git" />

  <js-module src="assets/js/PrintPlugin.js" name="PrintPlugin">
    <clobbers target="window.PrintPlugin" />
  </js-module>
</plugin>

<icon src="assets/img/icon.png" />

</widget>